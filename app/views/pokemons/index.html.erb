<body>
  <div class="pokedex-title">
    <p id="title">Pokedex 151</p>
  </div>

  <div id="popup">This is my fucking name</div>

  <div class="sprites">
    <% @pokemons.each do |pokemon| %>
      <% if pokemon.number < 152 %>
        <%= link_to image_tag("http://img.pokemondb.net/sprites/black-white/normal/" +  pokemon.name.tr('[]', '').gsub(/[""]/, '') + ".png"), pokemon_path(pokemon), :id => "sprite" %>
      <% end %>
    <% end %>
  </div>
</body>
